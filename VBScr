msgbox a						05/02/1985
a=dateadd("h",2,time)
msgbox a						3:31:16 AM
a=dateadd("yyyy",-2,#04/30/1985#)
msgbox a						04/30/1983
a=dateadd("m",-2,#04/30/1985#)
msgbox a						02/28/1985
a=dateadd("d",-2,#04/30/1985#)
msgbox a						04/28/1985
a=dateadd("h",-2,time)
msgbox a						12/29/1899 11:32:21 PM
